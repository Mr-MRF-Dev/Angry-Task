{
  "name": "angry-task",
  "version": "0.1.1",
  "scripts": {
    "prepare": "husky",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c production --base-href 'https://mr-mrf-dev.github.io/Angry-Task/'",
